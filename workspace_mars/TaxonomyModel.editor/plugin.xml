<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated taxonomyModel -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="taxonomy.presentation.TaxonomyModelWizardID"
            name="%_UI_TaxonomyModelWizard_label"
            class="taxonomy.presentation.TaxonomyModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/TaxonomyModelFile.gif">
         <description>%_UI_TaxonomyModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated taxonomyModel -->
      <editor
            id="taxonomy.presentation.TaxonomyEditorID"
            name="%_UI_TaxonomyEditor_label"
            icon="icons/full/obj16/TaxonomyModelFile.gif"
            extensions="taxonomy"
            class="taxonomy.presentation.TaxonomyEditor"
            contributorClass="taxonomy.presentation.TaxonomyActionBarContributor">
      </editor>
   </extension>

</plugin>
