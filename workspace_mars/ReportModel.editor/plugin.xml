<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated reportModel -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="report.presentation.ReportModelWizardID"
            name="%_UI_ReportModelWizard_label"
            class="report.presentation.ReportModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ReportModelFile.gif">
         <description>%_UI_ReportModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated reportModel -->
      <editor
            id="report.presentation.ReportEditorID"
            name="%_UI_ReportEditor_label"
            icon="icons/full/obj16/ReportModelFile.gif"
            extensions="report"
            class="report.presentation.ReportEditor"
            contributorClass="report.presentation.ReportActionBarContributor">
      </editor>
   </extension>

</plugin>
